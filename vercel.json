{
    "version": 2,
    "builds": [
        { "src": "/public/index.php", "use": "vercel-php@0.7.2" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://stream-jg.vercel.app"
    }
}
